		partialRootPaths {
			10 = EXT:theme_t3kit/Resources/Private/Extensions/Solr/HeaderMiddleSearch/Partials/
		}
		layoutRootPaths {
			10 = EXT:theme_t3kit/Resources/Private/Extensions/Solr/HeaderMiddleSearch/Layouts/
		}
    }
[global]
